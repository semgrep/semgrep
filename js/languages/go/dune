(executables
 (names Parser)
 (libraries semgrep_js_shared integers_stubs_js ctypes_stubs_js
   parser_go.menhir parser_go.tree_sitter parser_go.ast_generic)
 (modes js)
 (js_of_ocaml
  (javascript_files semgrep.js)
  (flags --pretty --no-inline))
 (preprocess
  (pps js_of_ocaml-ppx)))

(rule
 (targets semgrep.js)
 (action
  (with-stdout-to
   %{targets}
   (run ../shared/generate-semgrep-js.sh go))))
