{
  "rules": [
    {
      "id": "test",
      "languages": ["python"],
      "all": [
        {
          "pattern": "print($BELOW_SIGNED_INT32, $ABOVE_SIGNED_INT32)",
          "where": [
            {
              "metavariable-comparison": {
                "comparison": "int($BELOW_SIGNED_INT32) < 2147483647 and int($ABOVE_SIGNED_INT32) > 2147483647"
              }
            }
          ]
        }
      ],
      "message": "test",
      "severity": "ERROR"
    }
  ]
}
