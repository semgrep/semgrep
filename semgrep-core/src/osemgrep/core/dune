; Various definitions used throughout osemgrep, the OCaml reimplementation
; of the original Python semgrep.
;
; Eventually, this will be merged with the /core library from semgrep-core

(library
  (name osemgrep_core)
  (wrapped false)
  (libraries
    uri
    commons        ; pfff
    semgrep_utils  ; semgrep-core
    semgrep_core
  )
 (inline_tests)
 (preprocess
   (pps
     ppx_profiling
     ppx_deriving.show
     ppx_deriving.eq
     ppx_hash
     ppx_inline_test
     ppx_assert
   )
 )
)
