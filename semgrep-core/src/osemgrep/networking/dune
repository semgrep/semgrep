; This library should contain all the entry points to access the internet.
(library
  (name osemgrep_networking)
  (wrapped false)
  (libraries
    lwt
    ; !!osemgrep_networking should be the only lib in semgrep using cohttp!!
    cohttp cohttp-lwt-unix

    profiling
  )
 (preprocess
   (pps
     ppx_profiling
     ppx_deriving.show
     ppx_deriving.eq
     ppx_hash
   )
 )
)
