(library
  (public_name semgrep_api)
  (libraries
     atdgen-runtime
     commons
     pfff-lang_GENERIC_base
  )
  (wrapped false)
  (preprocess
   (pps
     ppx_profiling
     ppx_deriving.show
     ppx_deriving.eq
   ))
)

(rule
 (targets ast_generic_v0_j.ml ast_generic_v0_j.mli)
 (deps    ast_generic_v0.atd)
 (action  (run atdgen -j -j-std %{deps})))

(rule
 (targets ast_generic_v0_t.ml ast_generic_v0_t.mli)
 (deps    ast_generic_v0.atd)
 (action  (run atdgen -t %{deps})))
